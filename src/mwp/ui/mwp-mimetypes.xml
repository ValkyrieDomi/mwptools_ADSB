<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">

  <mime-type type="application/vnd.mw.mission">
    <comment>Multiwii mission</comment>
    <glob pattern="*.mission"/>
  </mime-type>

  <mime-type type="application/vnd.blackbox.log">
    <comment>Blackbox log</comment>
    <magic>
      <match type="string" offset="0" value="H Product:Blackbox"/>
    </magic>
    <glob pattern="*.TXT"/>
    <glob pattern="*.txt"/>
    <glob pattern="*.bbl"/>
    <glob pattern="*.BBL"/>
  </mime-type>

  <mime-type type="application/vnd.mwp.log">
    <comment>Mwp telemetry log</comment>
    <magic>
      <match type="string" offset="0" value='{"type":'/>
    </magic>
    <glob pattern="*.log"/>
  </mime-type>

  <mime-type type="application/vnd.mwp.json.mission">
    <comment>Multiwii mission</comment>
    <magic>
      <match type="string" offset="0" value='{"missions":'/>
    </magic>
    <glob pattern="*.json"/>
  </mime-type>

  <mime-type type="application/vnd.otx.telemetry.log">
    <comment>OTX telemetry log</comment>
    <magic>
      <match type="string" offset="0" value='Date,Time,'/>
    </magic>
    <glob pattern="*.csv"/>
  </mime-type>

</mime-info>
